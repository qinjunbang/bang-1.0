    section.hobbies
        div.container
            div.title.text-center
                h2 兴趣·爱好
                p.sub-title Hobbies and Interests
            div.hobbies-content
                ul.list-unstyled.hobbies-list
                    li.col-sm-3
                        a(href="javascript: void(0);")
                            div.img
                                img(src="images/www/music.jpg" alt="听音乐").img-responsive.mr-auto
                            p.text.f18.text-center 听音乐
                    li.col-sm-3
                        a(href="javascript: void(0);")
